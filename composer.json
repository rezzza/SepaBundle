{
    "name": "rezzza/sepa-bundle",
    "description": "Export model at Sepa format",
    "keywords": ["payment", "sepa", "symfony", "bundle"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/rezzza/SepaBundle",
    "license": "MIT",
    "authors": [{
        "name": "Community contributors",
        "homepage": "https://github.com/rezzza/Sepa/contributors"
    }],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.0|~3.0",
        "jms/serializer": ">=0.10"
    },
    "autoload":     {
        "psr-0": { "Rezzza\\SepaBundle": "" }
    },
    "target-dir": "Rezzza/SepaBundle",
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
